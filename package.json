{
  "name": "linter-flake8",
  "main": "./lib/main",
  "version": "1.13.0",
  "description": "Atom linter plugin for Python, using flake8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-flake8"
  },
  "dependencies": {
    "atom-linter": "^4.6.1",
    "atom-package-deps": "^4.0.1",
    "fs-plus": "^2.8.1"
  },
  "devDependencies": {
    "coffeelint": "^1.15.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-react": "^5.0.1"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": 0
    },
    "extends": "airbnb/base",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
