{
  "name": "linter-flake8",
  "main": "./lib/main",
  "version": "1.10.1",
  "description": "Atom linter plugin for Python, using flake8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-flake8"
  },
  "dependencies": {
    "atom-linter": "^4.3.4",
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "coffeelint": "^1.14.2",
    "eslint": "^1.10.3",
    "babel-eslint": "^4.1.6",
    "eslint-config-airbnb": "^5.0.0"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
